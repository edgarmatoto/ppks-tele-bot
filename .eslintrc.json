{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "linebreak-style":"off",
        "no-underscore-dangle":"off",
        "no-unused-vars":"off",
        "no-console":"off",
        "class-methods-use-this":"off",
        "import/extensions":"off",
        "max-len":"off",
        "padded-blocks":"off",
        "camelcase":"off"
    }
}
